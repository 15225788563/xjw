.return{
  padding: 0;margin: 0;
  height: 1500rpx;
  background-color: rgb(235, 232, 232);
}
.header{
  background-color: #09c15f;
  height: 128rpx;
  padding-top:25rpx; 
}
.left image{
  position: absolute;
  top: 10rpx;
  left: 10rpx;
}
.header .left{
  position: relative;
  height: 150rpx;
  line-height: 150rpx;
  margin: 0 100rpx;
}
.header .cent{
  margin-left: 25rpx;
  font-weight: bold;
  font-size: 42rpx;
}
.rebasket{
  border: 1px solid #fff;
  border-radius: 15rpx;
  width: 90%;
  background-color: #fff;
  margin: 15rpx auto;
}
.rebasket .name{
  border-left: 15rpx solid #09c15f;
  padding-left:12rpx; 
  margin: 25rpx;
}
.rebasket .numTime{
  border-top: 1px solid #ccc;
  height: 80rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  color: #ccc;
  padding: 10rpx 55rpx;
  border-bottom: 1px solid #ccc;
}
.Details{
  display: flex;
}
.Details .det{
  font-size: 24rpx;
  width: 100rpx;
  color: #09c15f;
  padding-left : 55rpx;
  height: 80rpx;
  line-height: 80rpx;
}
.Details .money{
  font-size: 28rpx;
  margin: 20rpx 0rpx 0 350rpx;
}
