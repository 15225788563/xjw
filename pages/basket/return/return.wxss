.return{
  padding: 0;margin: 0;
  height: auto;
  background-color: rgb(235, 232, 232);
}
.header{
  background-color: #09c15f;
  height: 128rpx;
  padding-top:25rpx; 
}
.left image{
  position: absolute;
  top: 58rpx;
  left: -50rpx;
}
.header .left{
  position: relative;
  height: 150rpx;
  line-height: 150rpx;
  margin: 0 100rpx;
  font-size: 28rpx;
}
.header .cent{
  margin-left: 125rpx;
  font-size: 36rpx;
}
.rebasket{
  border: 1px solid #fff;
  border-radius: 15rpx;
  width: 90%;
  background-color: #fff;
  padding: 5rpx;
  margin: 15rpx auto;
}
.rebasket .name,.rename{
  border-left: 15rpx solid #09c15f;
  padding-left:12rpx; 
  margin: 25rpx;
}
.rebasket .numTime{
  border-top: 1px solid #ccc;
  height: 80rpx;
  line-height: 45rpx;
  font-size: 24rpx;
  color: #ccc;
  padding: 10rpx 55rpx;
  border-bottom: 1px solid #ccc;
}
.Details{
  display: flex;
  position: relative;
}
.Details .det{
  font-size: 24rpx;
  width: 100rpx;
  color: #09c15f;
  padding-left : 55rpx;
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
}
.Details .money{
  color: #f00;
  position: absolute;
  top: 20rpx;
  right: 45rpx;
}
.Details .remoney{
  font-size: 28rpx;
  line-height: 85rpx;
  display: inline;
  position: absolute;
  right: 50rpx;
}
