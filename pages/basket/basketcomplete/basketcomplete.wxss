Page{
  background: #f5f5f5;
}
.fixed{
  position: fixed;
  top: 0;
  width: 100%;
}
.harder{
  padding: 0 300rpx 0 30rpx;
  height: 128rpx;
  background: #33da82;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-top: 20rpx;
}
.harder .harders{
  display: flex;
  align-items: center;
}
.harder .harders .txt{
  margin-left: 10rpx;
  font-size: 28rpx;
}
.harder image{
  width: 38rpx;
  height: 38rpx;
}
.body{
  margin-top: 160rpx;
  padding: 0 30rpx;
}

.body .list{
  height: 335rpx;
  background: #fff;
  border-radius: 20rpx;
  padding: 20rpx 30rpx 0 25rpx;
  margin-bottom: 10rpx;
}
.body .list .listharder{
  display: flex;
  justify-content: space-between;
}
.body .list .listbody{
  margin-top: 10rpx;
  width: 100%;
  height: 210rpx;
  display: flex;
  justify-content: space-around;
  border-bottom: 2px solid #eeeeee;
}
.body .list .listbody .Basketnumber{
  font-size: 27rpx;
  width: 157rpx;
}
.body .list .listbody .block{
  flex: 1;
  display: flex;
  flex-wrap:wrap;
  justify-content: space-between;
}
.body .list .listbody .block view{
  color: #ccc;
  font-size: 25rpx;
  width: 33.3%;
}

.body .listfooter{
  height: 57rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 25rpx;
}
.body .listfooter .rmb{
  font-size: 28rpx;
}

.footer{
  background: #fff;
  height: 175rpx;
  width: 100%;
  position: fixed;
  bottom: 0;
  text-align: center;
  padding-top: 20rpx;
}

.footer image{
  width: 44rpx;
  height: 44rpx;
}
.footer .Refresh{
  display: flex;
  justify-content: center;
  align-items: flex-end;
  color:#32db80;
  margin-top: 50rpx;
}