.Management{
  padding: 0;margin: 0;
  height: auto;
  background: rgb(235, 232, 232);
}
.header{
  background-color: #09c15f;
  height: 128rpx;
  padding-top:25rpx; 
}
.header .left{
  height: 150rpx;
  line-height: 150rpx;
  margin: 0 100rpx;
}
.header .cent{
  margin-left: 45rpx;
  font-weight: bold;
  font-size: 42rpx;
}
.list{
  width: 95%;
  height: 350rpx;
  background-color: #fff;
  margin: 15rpx auto;
  border-radius: 15rpx;
}
.list .top{
  padding: 24rpx 28rpx;
  display: flex;
  font-size: 26rpx;
  font-weight: bold;
}
.return{
  flex: 1;
}
.center{
  margin-left: 28rpx;
  font-size: 24rpx;
  height: 200rpx;
  width: 100%;
  display: flex;
}
.center .text{
  font-size: 22rpx;
  margin: 10rpx 50rpx 10rpx 20rpx;
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  color: #999;
}
.text view{
  margin: 15rpx;
  flex: 1;
}
.bottom{
  border-top:1rpx solid rgb(235, 232, 232); 
  font-size: 24rpx;
  display: flex;
  height: 60rpx;
  line-height: 60rpx;
  margin: 0 28rpx;
}
.bottom .numb{
  margin:0 20rpx 0 100rpx;
  font-size: 20rpx;
}
.bottom .total{
  font-size: 20rpx;
}
.total text{
  font-size: 26rpx;
  font-weight: bold;
}


